    "ibm_array_mk_DRAID":{
        "prefix":["ibm_array, draid, raid"],
        "body":[
            "mkdistributedarray -level ${1|raid6,raid1|} -driveclass ${2:driveclass_id} -drivecount ${3:2=><=128} -encrypt ${4|no,yes|} ${5:mdiskgrp_name}"
        ],
        "description": " Create a distributed array.\n There is some more parameters, but these are optional\n Driveclass_id must be a numeric value specified with the \"lsdriveclass\" command.\n If there is only one drive class, the id is 0"
    }
